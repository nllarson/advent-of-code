    "test:2017": "jest --config 2017/jest.config.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "dotenv": "~4.0.0",
    "husky": "~0.15.0-beta.15",
    "jest": "~21.2.1",
    "lint-staged": "~6.0.0",
    "markdown-magic": "~0.1.19",
    "prettier": "~1.8.2"
  },
  "dependencies": {
    "axios": "~0.17.1"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets":["env"]
  }
